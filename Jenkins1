       stage('GetCode'){
          
                git 'https://github.com/sabrine924/ProjetDevOps.git'
            
         }        
stage('Checkout') {
        checkout scm
    }

    stage('Build'){
        sh "mvn clean install"
    }
    
    
    
    
    }
    
    
    
    
    }
    
    
